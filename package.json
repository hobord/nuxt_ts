{
  "name": "nuxt_ts",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/component-cache": "^1.1.2",
    "@nuxtjs/pwa": "^2.0.8",
    "cross-env": "^5.1.6",
    "nuxt-edge": "^2.0.0-25471736.3b2ed03",
    "nuxt-property-decorator": "^1.2.0",
    "vuex-class": "^0.3.1"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "serve": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "test:unit": "cross-env TEST=unit jest",
    "test:e2e": "cross-env TEST=e2e ava ./test/e2e/*.js --verbose",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts}",
      "**/*.{vue}",
      "test/spec/*",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "bin/jest",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "@vue/server-test-utils": "^1.0.0-beta.18",
    "@vue/test-utils": "^1.0.0-beta.18",
    "autoprefixer": "^8.6.2",
    "ava": "^0.25.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-vue-preprocessor": "^1.4.0",
    "source-map-support": "^0.5.6",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.1",
    "vue-jest": "^2.6.0"
  }
}
